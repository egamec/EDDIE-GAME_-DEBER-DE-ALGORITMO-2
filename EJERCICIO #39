Algoritmo vector_primo
	Escribir "ingrese el numero de la secuencia"
	Leer sec
	Dimension vector[sec]
	Escribir " "
	Para i<-1 hasta sec Hacer
		Escribir "Introduzca vector[",i,"]:"
		Leer vector[i]
	FinPara
	Para i<-1 hasta sec Hacer
		cont<-0
		Para j<-1 Hasta vector[i] Hacer
			si vector[i]%j = 0 Entonces
				cont<-cont+1
			FinSi
		FinPara
		Si cont = 2 Entonces
			Escribir "Primo Encontrado:",vector[i]
		FinSi
	FinPara
	
FinAlgoritmo
