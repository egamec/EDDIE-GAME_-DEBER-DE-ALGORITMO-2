Algoritmo suma_c_almacenado
	Dimension a[20];
	Dimension b[20];
	Dimension c[20];
	i<-0;
	Escribir "Deber introducir 30 valores";
	Para i<-1 hasta 15 Hacer
		Escribir "Introduzca el numero del vector a[",i,"]:"
		Leer a[i];     
		Escribir "Introduzca el numero del vector  b[",i,"]:"
		Leer b[i];
		c[i]<-a[i]+b[i];Escribir""
	FinPara
	p=1
	Para i<-1 hasta 15 Hacer
		Escribir "en la posicion ",p, " de c se almacena"," ","[",c[i],"]"
		p=p+1
	FinPara 
FinAlgoritmo
